    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "ng-alain": "^9.2.1",
    "ng-alain-codelyzer": "^0.0.1",
    "@delon/testing": "^9.2.1",
    "antd-theme-generator": "^1.1.9",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "lint-staged": "^10.1.2",
    "husky": "^4.2.3",
