    "@delon/cache": "^9.2.1",
    "@delon/form": "^9.2.1",
    "@delon/mock": "^9.2.1",
    "@delon/theme": "^9.2.1",
    "@delon/util": "^9.2.1",
    "@delon/chart": "^9.2.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
